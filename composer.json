{
    "name": "ivanwilliammd/satusehat-integration",
    "description": "Build SATUSEHAT FHIR Object in Easy Way",
    "keywords": [
        "ivanwilliammd",
        "satusehat-integration"
    ],
    "homepage": "https://github.com/ivanwilliammd/satusehat-integration",
    "license": "MIT",
    "authors": [
        {
            "name": "ivanwilliammd",
            "email": "ivanwilliam.md@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0|^8.1",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/config": "^8.0|^9.0|^10.00|^11.00",
        "illuminate/database": "^8.0|^9.0|^10.00|^11.00",
        "illuminate/support": "^8.0|^9.0|^10.00|^11.00",
        "jeroenzwart/laravel-csv-seeder": "^1.6",
        "phpseclib/phpseclib": "^3.0",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "suggest": {
        "laravel/pint": "Minimalist Opionated Code Formatting Extension for PHP 8.0+",
        "pestphp/pest": "Pest PHP Testing Framework for PHP 8.0+"
    },
    "autoload": {
        "psr-4": {
            "Satusehat\\Integration\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Satusehat\\Integration\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Satusehat\\Integration\\SatusehatIntegrationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
